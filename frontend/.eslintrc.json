{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-console": "warn",
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "warn"
	}
}
